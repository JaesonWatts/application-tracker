<%= form_with model: open_position, local: true do |f| %>
  <%= render partial: 'layouts/errors', locals: {object: open_position} %>

  <br><br>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <br><br>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <br><br>
  <%= f.label :compensation %>
  <%= f.text_field :compensation %>
  <br><br>
  <%= f.label :remote %>
  <%= f.check_box :remote %>
  <br><br>
  <%= f.label :status %>
  <%= f.text_field :status %>
  <br><br>
  <%= f.label :notes %>
  <%= f.text_field :notes %>
  <br><br>
  <%= f.label :company_id %>
  <%= f.collection_select :company_id,Company.order(:name), :id, :name, include_blank: true %>
  <br><br>

  <%= f.submit "Submit", disable_with: 'Submitting...' %>

<% end %>